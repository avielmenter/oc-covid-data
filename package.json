    "build": "tsc",
    "start:dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "start": "node ./dist/index.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avielmenter/oc-covid-data.git"
  },
