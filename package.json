  "bugs": {
    "url": "https://github.com/avielmenter/oc-covid-data/issues"
  },
  "homepage": "https://github.com/avielmenter/oc-covid-data#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node-fetch": "^2.5.7",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.3",
    "immutable": "^4.0.0-rc.12",
    "node-fetch": "^2.6.0"
  }
